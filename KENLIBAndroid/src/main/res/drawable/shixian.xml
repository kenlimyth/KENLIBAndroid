<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line">

    <!-- 显示一条虚线，破折线的宽度为dashWith，破折线之间的空隙的宽度为dashGap，
    当dashGap=0dp时，为实线 -->
    <stroke android:dashWidth="5dp"
    android:dashGap="0dp"
    android:color="@color/hs"
    android:width="1dp"
    />

</shape>
